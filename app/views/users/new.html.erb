<h1>New Users</h1>
<%= form_for @user do |f|%>

    <label>First Name </label>
    <%= f.text_field :first_name %> <br><br>
    <label> Last Name </label>
    <%= f.text_field :last_name %> <br><br>
    <label> Age </label>
    <%= f.number_field :age %> <br><br>
    <label> Bio </label>
    <%= f.text_field :bio %> <br><br>
    <label> Interests </label>
    <%= f.text_field :interest %> <br><br>
    <label> Image </label>
    <%= f.text_field :img_url %> <br><br>
    <label> UserName </label>
    <%= f.text_field :username %> <br><br>
    <label> Password </label>
    <%= f.password_field :password %> <br><br>
    

    <%= f.submit %>

    <% end %>

<% if flash[:errors]%>
        <% flash[:errors].each do |error|%>
        <li><%=error%></li>
        <%end%>
<%end%>



