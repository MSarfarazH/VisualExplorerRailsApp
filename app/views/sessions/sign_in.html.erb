<h1 style="color:red"><%=flash[:error]%></h1>

<%= form_with model: @user, :url => url_for(:controller => 'sessions', :action => 'create')do | f |%>

<%= f.label :username %>
<%= f.text_field :username %>
<br/>
<%= f.label :password %>
<%= f.password_field :password %>
<br/>
<%= f.submit "Sign In" %>

<% end %>

<%=link_to "Don't have an account? Create one!", '/users/new' %>